    "import torch\n",
    "from torch import nn\n",
    "def create_mlp_model() -> nn.Module:\n",
    "    my_model = nn.Sequential(\n",
    "        nn.Flatten(),\n",
    "        nn.Linear(28 * 28, 256),\n",
    "        nn.ReLU(),\n",
    "        nn.Linear(256, 10)\n",
    "    )\n",
    "    return my_model"
   ]
  }
